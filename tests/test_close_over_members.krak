import io:*

fun runFunc(func: fun():void) {
    func()
}

obj ToClose {
    var data: int
    fun print4() {
        println(4)
    }
    fun testMethod() {
        runFunc(fun() { print4(); } )
    }
    fun testVariable() {
        data = 7
        runFunc(fun() { println(data); } )
    }
}

fun main():int {
    var it: ToClose
    it.testMethod()
    it.testVariable()
    return 0
}

