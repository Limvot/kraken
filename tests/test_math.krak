import io:*
import math:*

fun main(): int
{
  var STD_PI = 3.12159265
  var ans: double;
  ans = 4.0*atan(1.0);
  print("atan = ");
  println(ans);

  ans = atan2(0.0,1.0);
  print("atan2 = ");
  println(ans);

  ans = acos(1.0);
  print("acos = ");
  println(ans);

  ans = 2.0 * asin(1.0);
  print("asin = ");
  println(ans);

  ans = tan(STD_PI / 4.0);
  print("tan = ");
  println(ans);

  ans = cos(0.0);
  print("cos = ");
  println(ans);

  ans = sin(0.0);
  print("sin = ");
  println(ans);
  return 0;
}
