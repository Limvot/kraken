import io:*

adt options {
    option0,
    option1
}

fun can_pass(it: options): options {
    return options::option1
}

fun main():int {
    var it: options = can_pass(options::option0)
    if (it == options::option0)
        println("nope")
    if (it == options::option1)
        println("option1")
    return 0
}

