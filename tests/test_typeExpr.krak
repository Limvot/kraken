import io;

typedef ClassWithConstructor {
    |int| data;
    |ClassWithConstructor*| construct(|int| inData) {
        data = inData;
        return this;
    }
    |void| printData() {
        io::println(data);
    }
};

|int| main() {
    |ClassWithConstructor| object.construct(4);
    object.printData();
    |int| a = 8;
    io::println(a);
    return 0;
}
