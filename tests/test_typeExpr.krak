import simple_print;

obj ClassWithConstructor {
    var data: int;
    fun construct(inData: int): *ClassWithConstructor {
        data = inData;
        return this;
    }
    fun printData(): void {
        simple_print::println(data);
    }
};

fun main(): int {
     var object.construct(4): ClassWithConstructor;
    object.printData();
    var a :int = 8;
    simple_print::println(a);
    return 0;
}
