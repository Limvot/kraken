import io:*;

typedef firstObject {
	var objectNum: int;
	var other: int;
	fun print(): void {
		print(other);
	}
	fun printInd(): void {
		print();
	}
};

typedef Int int;

var aliasNum: Int;

fun main(): int {
	var wooObject: firstObject;
	wooObject.objectNum = 7;
	print(wooObject.objectNum);
	var objPtr: firstObject* = &wooObject;
	objPtr->objectNum = 42;
	print(objPtr->objectNum);
	print("\n");
	objPtr->other = 1337;
	objPtr->printInd();
	print("\n");
}
