import io;


obj TemplateTest<T,J> {
	var a: T;
	var b: J;
	fun print(): void {
        io::print("a: ");
        io::print(a);
        io::print("\n");
        io::print("b: ");
        io::print(b);
        io::print("\n");
	}
};

fun main(): int {

	var test: TemplateTest<int, char*>;
	var test2: TemplateTest<char*, char*>;
	test.a = 24;
	test.b = "Hello World";
	test2.a = "Pi incoming";
	test2.b = "3.14159 - Fooled you! txt pi. C is being weird with floats. Not a Kraken problem. Ahh Well.";

	test.print();
	test2.print();

	return 0;
}
