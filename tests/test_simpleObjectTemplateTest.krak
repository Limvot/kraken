import io;


obj TemplateTest<T> {
	var a: int;
	var b: T;
	fun print(): void {
        io::print("a: ");
        io::print(a);
        io::print("\n");
        io::print("b: ");
        io::print(b);
        io::print("\n");
	}
};

fun main(): int {

	var test: TemplateTest<int>;
	var test2: TemplateTest<char*>;
	test.a = 5;
	test.b = 7;
	test2.a = 9;
	test2.b = "Hello Templates!";

	test.print();
	test2.print();

	return 0;
}
